it('test increment and decrement', () => {


  let initialState = 0;

  const increment = () => {
    console.log('calling increment');
    initialState++;
  };

  const decrement = () => {
    initialState--;
  };

  const wrapper = /* istanbul ignore next */(0, /* istanbul ignore next */_enzyme. /* istanbul ignore next */shallow)( /* istanbul ignore next */_react2.default.createElement( /* istanbul ignore next */_CounterComponent2. /* istanbul ignore next */default, /* istanbul ignore next */{ counter: initialState, decrement: decrement, increment: increment, __source: { fileName: _jsxFileName, lineNumber: 33 }, __self: undefined }));


  const incrementButton = wrapper.find('.increment-counter');
  const decrementButton = wrapper.find('.decrement-counter');

  incrementButton.simulate('click');
  /* istanbul ignore next */(0, /* istanbul ignore next */_chai. /* istanbul ignore next */expect)(initialState).equals(1);
  decrementButton.simulate('click');
  /* istanbul ignore next */(0, /* istanbul ignore next */_chai. /* istanbul ignore next */expect)(initialState).equals(2);

});